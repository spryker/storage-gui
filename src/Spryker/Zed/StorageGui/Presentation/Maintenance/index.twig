{% extends '@Gui/Layout/layout.twig' %}

{% set widget_title = 'Storage overview' %}

{% block head_title widget_title | trans %}

{% block content %}
    {% embed '@Gui/Partials/widget.twig' %}
        {% block widget_content %}
            <dl class="row">
                <dt class="col-3">{{ "Number of entries:" | trans }}</dt>
                <dd class="col-9"><a href="/storage-gui/maintenance/list">{{ totalCount | executeFilterIfExists('formatInt', app.locale) }}</a></dd>
            </dl>
        {% endblock %}
    {% endembed %}
{% endblock %}
